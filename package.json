{
  "name": "naive-blog",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "cross-env NEXT_PUBLIC_COMMIT_HASH=$(git rev-parse HEAD) INIT_CWD=$PWD NODE_OPTIONS='--inspect --trace-deprecation' next dev --turbopack",
    "dev:without-tp": "cross-env NEXT_PUBLIC_COMMIT_HASH=$(git rev-parse HEAD) INIT_CWD=$PWD NODE_OPTIONS='--inspect --trace-deprecation' next dev",
    "build": "cross-env NODE_OPTIONS='--experimental-json-modules' node src/utils/prebuild.mjs && cross-env NEXT_PUBLIC_COMMIT_HASH=$(git rev-parse HEAD) INIT_CWD=$PWD next build",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir app --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky"
  },
  "dependencies": {
    "@headlessui/react": "2.2.2",
    "@million/lint": "^1.0.14",
